<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="projectp4.studio.com.gerenciador_universitario">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <data android:scheme="projectp4.studio.com.gerenciador_universitario"/>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.BROWSABLE"/>
            </intent-filter>
        </activity>
        <activity android:name=".AddMat" />
        <activity android:name=".Ranking" />
        <activity android:name=".AddNotas" />
        <activity android:name=".VerNotas" />
        <activity android:name=".AddFaltas" />
        <activity android:name=".SituAtual"></activity>
    </application>

</manifest>